<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003"
 		DefaultTargets="GCTests;IPLookupSafe;IPLookup;Baseline;BaselineSafe;BaselineT;BaselineTSafe;IPLookupT;IPLookupTSafe;IdleThr;IdleThrSafe">
	<ItemGroup>
		<BaselineGC Include="chain-same-core/BaselineGC1.cs" />
		<FixedGC Include="chain-same-core/FixedGC1.cs" />
		<DynamicGC Include="chain-same-core/DynamicGC1.cs" />
		<IPLookup Include="chain-same-core/IPLookup.cs" />
		<IPLookupThr Include="chain-same-core/IPLookupThr.cs" />
		<Baseline Include="chain-same-core/Baseline.cs" />
		<BaselineThr Include="chain-same-core/BaselineThr.cs" />
		<IdleThr Include="chain-same-core/IdleThr.cs" />
		<Framework Include="framework/*.cs" />
	</ItemGroup>
	<PropertyGroup>
		<OutputPath>bin/</OutputPath>
		<BaselineGCOutput>BaselineGC.exe</BaselineGCOutput>
		<FixedGCOutput>FixedGC.exe</FixedGCOutput>
		<DynamicGCOutput>DynamicGC.exe</DynamicGCOutput>
		<IPLookupOutput>IPLookup.exe</IPLookupOutput>
		<IPLookupSafeOutput>IPLookupSafe.exe</IPLookupSafeOutput>
		<BaselineOutput>Baseline.exe</BaselineOutput>
		<BaselineSafeOutput>BaselineSafe.exe</BaselineSafeOutput>
		<BaselineThrOutput>BaselineT.exe</BaselineThrOutput>
		<BaselineSafeThrOutput>BaselineSafeT.exe</BaselineSafeThrOutput>
		<IPThrOutput>IPLookupT.exe</IPThrOutput>
		<IPSafeThrOutput>IPSafeT.exe</IPSafeThrOutput>
		<IdleThrOutput>IdleThr.exe</IdleThrOutput>
		<IdleSafeThrOutput>IdleSafeThr.exe</IdleSafeThrOutput>
	</PropertyGroup>
	<Target Name="GCTests">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(BaselineGC);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(BaselineGCOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" />
		<Csc Sources="@(FixedGC);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(FixedGCOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" />
		<Csc Sources="@(DynamicGC);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(DynamicGCOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" />
	</Target>
	<Target Name="IPLookupSafe">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(IPLookup);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(IPLookupSafeOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" 
		 	 DefineConstants="UNIQUE_CHECK"/>
	</Target>
	<Target Name="IPLookup">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(IPLookup);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(IPLookupOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" />
	</Target>
	<Target Name="BaselineSafe">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(Baseline);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(BaselineSafeOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" 
		 	 DefineConstants="UNIQUE_CHECK"/>
	</Target>
	<Target Name="Baseline">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(Baseline);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(BaselineOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" />
	</Target>
	<Target Name="BaselineTSafe">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(BaselineThr);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(BaselineSafeThrOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" 
		 	 DefineConstants="UNIQUE_CHECK"/>
	</Target>
	<Target Name="BaselineT">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(BaselineThr);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(BaselineThrOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" />
	</Target>
	<Target Name="IPLookupTSafe">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(IPLookupThr);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(IPSafeThrOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" 
		 	 DefineConstants="UNIQUE_CHECK"/>
	</Target>
	<Target Name="IPLookupT">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(IPLookupThr);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(IPThrOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" />
	</Target>
	<Target Name="IdleThrSafe">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(IdleThr);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(IdleSafeThrOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" 
		 	 DefineConstants="UNIQUE_CHECK"/>
	</Target>
	<Target Name="IdleThr">
		<MakeDir Directories="$(OutputPath)" Condition="!Exists('$(OutputPath)')" />
		<Csc Sources="@(IdleThr);@(Framework)" 
		 	 OutputAssembly="$(OutputPath)$(IdleThrOutput)" 
		 	 EmitDebugInformation="false" 
		 	 AllowUnsafeBlocks="true"
		 	 CheckForOverflowUnderflow="false"
		 	 Optimize="true" />
	</Target>
</Project>
